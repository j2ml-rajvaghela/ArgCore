    <script>
        $(function () {

            $("#sortable2").sortable();
            $(".BlockResize").resizable({
                resize: function (event, ui) {
                    updateContentSize($(this));
                },
                maxWidth: 727,
                minWidth: 727,
                stop: function () {
                    $(".savechangesbtn").removeClass("hidden");
                }
            });
            $("#sortable2").sortable({
                start: function (event, ui) {
                    ui.item.addClass('dragged-shadow');
                },
                stop: function (event, ui) {
                    ui.item.removeClass('dragged-shadow');
                }
            });
            $("#sortable2").disableSelection();
        });
        function updateContentSize(element) {
            var findwidget = $(element).find(".itemsBlock");
            if (findwidget !== "" && findwidget.length > 0) {
                var contEvntHistHeight = $(element).height();
                var newItemsBlockHeight = contEvntHistHeight - 50;
                $(findwidget).css("max-height", newItemsBlockHeight + "px");
            }
        }
        function ArrangeBlock(event, element) {
            if (event) {
                event.stopPropagation();
            }
            $('#SortBlock').trigger('click');
        }
    </script>
