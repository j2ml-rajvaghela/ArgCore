    <div class="modal modal-fullscreen fade" id="Sorting-modal" tabindex="-2" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body">
                    <div id="Soringparentdiv">
                        <button type="button" class="close" data-dismiss="modal">
                            <span aria-hidden="true">X</span>
                        </button>
                        <div class="headingBlock">
                            <span class="heading">Arrangement</span>
                        </div>
                        <div class="row" id="sortable2">
                            @if (Model.BOLAuditSorting != null && Model.BOLAuditSorting.ConvertedJson != null && Model.BOLAuditSorting.ConvertedJson.Count > 0)
                            {
                                foreach (var item in Model.BOLAuditSorting.ConvertedJson)
                                {
                                    var blockname = "";
                                    //if (Model != null && Model.BOLAuditSorting != null && (Model.BOLAuditSorting.CompanyID==1005 || Model.BOLAuditSorting.CompanyID==1018))
                                    if (Model != null && Model.BOLAuditSorting != null && (Model.BOLAuditSorting.DBName == "ClientPasha" || Model.BOLAuditSorting.DBName == "ClientAgility"))
                                    {
                                        blockname = Common.GetEnumDescription((Common.Contants.BOLSorting)Convert.ToInt32(item.FieldValue));
                                    }
                                    else if (Model != null && Model.BOLAuditSorting != null && Model.BOLAuditSorting.DBName == "ClientHellmann")
                                    {
                                        blockname = Common.GetEnumDescription((Common.Contants.ClientHellmanWidgets)Convert.ToInt32(item.FieldValue));
                                    }
                                    <div class="col-12 sortablediv ui-state-default" data-div="@item.FieldName" data-id="@Convert.ToInt32(item.FieldValue)"><i class="fa fa-arrows-v seticon"></i> @blockname</div>
                                }
                            }
                            else
                            {
                                //if (Model != null && Model.BOLAuditSorting != null && (Model.BOLAuditSorting.CompanyID==1005 || Model.BOLAuditSorting.CompanyID==1018))
                                if (Model != null && Model.BOLAuditSorting != null && (Model.BOLAuditSorting.DBName == "ClientPasha" || Model.BOLAuditSorting.DBName == "ClientAgility"))
                                {
                                    foreach (var datavalue in Enum.GetValues(typeof(Common.Contants.BOLSorting)))
                                    {
                                        var blockName = Common.GetEnumDescription((Common.Contants.BOLSorting)datavalue);
                                        <div class="col-12 sortablediv ui-state-default" data-div="@datavalue" data-id="@Convert.ToInt32(datavalue)"><i class="fa fa-arrows-v seticon"></i> @blockName</div>

                                    }
                                }
                                else if (Model != null && Model.BOLAuditSorting != null && Model.BOLAuditSorting.DBName == "ClientHellmann")
                                {
                                    foreach (var datavalue in Enum.GetValues(typeof(Common.Contants.ClientHellmanWidgets)))
                                    {
                                        var blockName = Common.GetEnumDescription((Common.Contants.ClientHellmanWidgets)datavalue);
                                        <div class="col-12 sortablediv ui-state-default" data-div="@datavalue" data-id="@Convert.ToInt32(datavalue)"><i class="fa fa-arrows-v seticon"></i> @blockName</div>

                                    }
                                }

                            }
                        </div>
                        <div class="row blocksortbutton">
                            <div id="SortButton" class="normalButton btn btn-primary" onclick="Applyarrangement(this)">
                                Apply
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
