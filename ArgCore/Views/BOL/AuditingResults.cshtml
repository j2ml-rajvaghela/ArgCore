                                            window.open("@(Common.MyRoot)BOL/AddBalanceDue?bolNo=@Model.AgilityBOLAuditResults.JobNumber&&IsNewBal=true", "_blank");

                                        }
                                    }
                                }
                            });
                        }
                    }
                    else {

                        window.open("@(Common.MyRoot)BOL/AddBalanceDue?bolNo=@Model.AgilityBOLAuditResults.JobNumber", "_blank");
                    }
                });
            </script>
        }
    }
